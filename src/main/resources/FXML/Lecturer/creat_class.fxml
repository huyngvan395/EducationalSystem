<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import org.kordamp.ikonli.javafx.FontIcon?>


<AnchorPane prefHeight="600.0" prefWidth="905.0" stylesheets="@../../CSS/Lecturer/creatclass.css"
            xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.educationalsystem.Controller.Lecturer.CreatClassController">
    <Text layoutX="306.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create Class Form">
        <font>
          <Font name="Arial" size="35.0"/>
        </font>
    </Text>
    <TextField fx:id="Id_class" layoutX="233.0" layoutY="111.0" prefHeight="26.0" prefWidth="472.0" promptText="ID_Class">
        <font>
          <Font name="Arial" size="20.0"/>
        </font>
    </TextField>
    <TextField fx:id="class_Name" layoutX="233.0" layoutY="189.0" prefHeight="26.0" prefWidth="472.0" promptText="Class_Name">
        <font>
          <Font name="Arial" size="20.0"/>
        </font>
    </TextField>
    <Button fx:id="creatClass" layoutX="421.0" layoutY="282.0" mnemonicParsing="false" styleClass="creat-button" text="Creat">
        <graphic>
          <FontIcon iconLiteral="fa-plus" iconSize="20"/>
        </graphic>
        <font>
          <Font name="Arial" size="20.0"/>
        </font>
    </Button>
    <Button fx:id="left_btn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" styleClass="left_button">
        <graphic>
          <FontIcon iconLiteral="fa-arrow-left" iconSize="25"/>
        </graphic>
    </Button>
</AnchorPane>
